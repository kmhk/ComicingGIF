GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/SCRecorder" "${PODS_ROOT}/Headers/Public/ZOZolaZoomTransition"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/SCRecorder" "$PODS_CONFIGURATION_BUILD_DIR/ZOZolaZoomTransition"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/SCRecorder" -isystem "${PODS_ROOT}/Headers/Public/ZOZolaZoomTransition"
OTHER_LDFLAGS = $(inherited) -ObjC -l"SCRecorder" -l"ZOZolaZoomTransition" -framework "AVFoundation" -weak_framework "GLKit" -weak_framework "Metal"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
